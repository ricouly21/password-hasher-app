{
  "expo": {
    "name": "password-hasher-app",
    "slug": "password-hasher-app",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.ricouly.passwordhasherapp",
      "buildNumber": "1.0",
      "config": {
        "usesNonExemptEncryption": false
      },
      "userInterfaceStyle": "automatic",
      "supportsTablet": true
    },
    "android": {
      "package": "com.ricouly.passwordhasherapp",
      "versionCode": 1
    }
  }
}
